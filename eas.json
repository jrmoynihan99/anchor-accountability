{
  "cli": {
    "version": ">= 16.18.0",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "channel": "development"
    },
    "preview": {
      "distribution": "internal",
      "channel": "preview"
    },
    "production": {
      "autoIncrement": true,
      "channel": "production",
      "android": {
        "buildType": "app-bundle"
      },
      "ios": {
        "preBuildCommand": "bash -c '/usr/libexec/PlistBuddy -c \"Set :CFBundleVersion $(/usr/libexec/PlistBuddy -c \"Print :CFBundleVersion\" ios/Anchor/Info.plist)\" ios/AnchorClip/Info.plist'"
      }
    }
  },
  "submit": {
    "production": {}
  }
}
