{
  "expo": {
    "name": "Anchor",
    "slug": "my-accountability-app",
    "version": "1.2.4",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "anchor",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": false,
      "bundleIdentifier": "com.jrmoynihan99.anchor",
      "associatedDomains": [
        "applinks:anchoraccountability.com"
      ],
      "entitlements": {
        "aps-environment": "production"
      },
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false,
        "NSUserNotificationUsageDescription": "We use notifications to alert you about new help requests, encouragements, and messages."
      }
    },
    "android": {
      "icon": "./assets/images/icon.png",
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.jrmoynihan99.anchor",
      "googleServicesFile": "./google-services.json",
      "permissions": [
        "android.permission.POST_NOTIFICATIONS"
      ],
      "intentFilters": [
        {
          "action": "VIEW",
          "autoVerify": true,
          "data": [
            {
              "scheme": "https",
              "host": "anchoraccountability.com",
              "pathPrefix": "/join"
            }
          ],
          "category": [
            "BROWSING",
            "DEFAULT"
          ]
        }
      ]
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#1b1d21"
        }
      ],
      "expo-font",
      "expo-notifications",
      "expo-localization",
      "expo-web-browser",
      [
        "@stripe/stripe-react-native",
        {
          "merchantIdentifier": "merchant.com.jrmoynihan99.anchor",
          "enableGooglePay": true
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "ddea5407-acdc-47f1-b3b8-60ac20360d51"
      }
    },
    "owner": "jrmoynihan99",
    "runtimeVersion": "1.2.1",
    "updates": {
      "url": "https://u.expo.dev/ddea5407-acdc-47f1-b3b8-60ac20360d51"
    }
  }
}
